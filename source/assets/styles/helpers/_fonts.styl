@font-face 
  font-family: 'Museo Sans Light';
  src: url('MuseoSansCyrl-300.eot');
  src: url('MuseoSansCyrl-300.eot?#iefix') format('embedded-opentype'),
  url('MuseoSansCyrl-300.woff') format('woff'),
  url('MuseoSansCyrl-300.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;

